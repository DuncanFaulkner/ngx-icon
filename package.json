{
  "name": "ngx-icon",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate": "svg-to-ts-constants"
  },
  "svg-to-ts": {
    "conversionType": "constants",
    "srcFiles": [
      "./projects/ngx-icon-lib/icons/**/*.svg"
    ],
    "outputDirectory": "./projects/ngx-icon-lib/src/lib",
    "interfaceName": "NgxIcon",
    "typeName": "ngxIcon",
    "prefix": "ic",
    "svgoConfig": {
      "plugins": [
        "cleanupAttrs"
      ]
    },
    "fileName": "ngx-icons",
    "additionalModelOutputPath": "./projects/ngx-icon-lib/src/lib",
    "compileSources": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular/cli": "^17.1.0",
    "@angular/compiler-cli": "^17.1.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.1.0",
    "svg-to-ts": "^11.0.1",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "20.11.0"
  }
}
